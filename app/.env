# Application
APP_NAME=JobMatcher
DEBUG=False
LOG_LEVEL=INFO

# Database
MONGODB_URI=mongodb://localhost:27017
MONGODB_DB=jobmatcher

# Matching Service
EMBEDDING_MODEL=all-MiniLM-L6-v2
CACHE_SIZE=1000
CACHE_TTL=3600
SEMANTIC_WEIGHT=0.6
RULE_WEIGHT=0.4

# Limits
MAX_CANDIDATES_PER_QUERY=1000
MAX_JOBS_PER_QUERY=1000
MIN_MATCH_SCORE=40.0